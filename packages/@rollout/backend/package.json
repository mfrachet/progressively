{
  "name": "@rollout/backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "db:gen:sqlite-schema": "cat ./prisma/sqlite.prisma ./prisma/shared-schema.prisma > ./prisma/schema.prisma",
    "db:gen:postgres-schema": "cat ./prisma/postgres.prisma ./prisma/shared-schema.prisma > ./prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --name init",
    "db:generate:schema": "prisma generate",
    "db:seed": "ts-node ./test/helpers/seed-run.ts",
    "db:cleanup": "ts-node ./test/helpers/cleanup-run.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.2",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.2",
    "@nestjs/platform-ws": "^8.4.0",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/websockets": "^8.2.2",
    "@prisma/client": "^2.20.1",
    "bcrypt": "^5.0.1",
    "camelcase": "^6.3.0",
    "cookie-parser": "^1.4.6",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "murmurhash3js": "^3.0.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/testing": "^8.2.2",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/node": "^14.17.34",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^26.6.3",
    "prettier": "^2.4.1",
    "prisma": "^3.5.0",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
